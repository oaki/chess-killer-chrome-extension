{
  "manifest_version": 2,
  "name": "Chess killer Chrome extension",
  "version": "1.2.3",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://lichess.org/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}